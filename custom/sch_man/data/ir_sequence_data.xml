<?xml version="1.0" encoding="utf-8" ?>
<odoo>

        <record id="sequence_student" model="ir.sequence">
            <field name="name">Seq Student</field>
            <field name="code">school.student.sequence</field>
            <field name="active">TRUE</field>
            <field name="prefix">S</field>
            <field name="padding">4</field>
<!--            <field name="number_next">1</field>-->
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_teacher" model="ir.sequence">
            <field name="name">Seq Teacher</field>
            <field name="code">school.teacher.sequence</field>
            <field name="active">TRUE</field>
            <field name="prefix">T</field>
            <field name="padding">4</field>
<!--            <field name="number_next">1</field>-->
            <field name="number_increment">1</field>
        </record>


</odoo>