<?xml version='1.0' encoding='UTF-8'?>
<odoo>

    <record id='hotel_room_booked_wizard_view' model='ir.ui.view'>
        <field name="name">Hotel Room Book</field>
        <field name="model">hotel.room.booked</field>
        <field name="arch" type="xml">
            <form string="Inquiry Details">
                <group>
                    <field name="hotel_room_ids"/>
                </group>
                <footer>
                    <button name="room_book" type="object" string="Search"
                            class="oe_right oe_highlight" />
                    <button string="Cancel" special="cancel" class="oe_right"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="hotel_room_book" model="ir.actions.act_window">
        <field name="name">Room Book</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">hotel.room.booked</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
        <field name="view_id" ref="hotel_room_booked_wizard_view"/>
        <field name="target">new</field>
    </record>


</odoo>